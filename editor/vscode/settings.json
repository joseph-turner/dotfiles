{
  "beautify.config": {
    "indent_size": 2,
    "wrap_line_length": 0,
    "html": {
      "indent_size": 2,
      "wrap_line_length": 0
    },
    "css": {
      "indent_size": 2
    }
  },
  "colorize.activate_variables_support_beta": true,
  "editor.fontFamily": "'FuraCode Nerd Font', monospace",
  "editor.fontWeight": "300",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.js": "javascriptreact",
    ".jenkins": "json",
    ".browserslistrc": "shellscript",
    "*.xml": "xml",
    ".env*": "env"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "gitlens.codeLens.scopes": [
    "document"
  ],
  "gitlens.keymap": "alternate",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.idSelector": "ignore",
  "scss.lint.zeroUnits": "warning",
  "scss.showErrors": true,
  "terminal.integrated.fontSize": 15,
  "window.zoomLevel": 0,
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "material-icon-theme.showWelcomeMessage": false,
  "[html]": {

    },
    "search.exclude": {
      "**/.git": true,
      "yarn.lock": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "eslint.packageManager": "yarn",
    "files.trimTrailingWhitespace": true,
    "emmet.includeLanguages": {
      "typescript": "typescriptreact",
      "javascript": "javascriptreact"
    },
    "editor.fontLigatures": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": true,
    "workbench.startupEditor": "welcomePage",
    "typescript.validate.enable": false,
    "javascript.validate.enable": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line-thin",
    "editor.fontSize": 14,
    "javascript.suggest.enabled": false,
    "material-icon-theme.activeIconPack": "react_redux",
    "oneDarkPro.bold": true,
    "oneDarkPro.vivid": true,
    "oneDarkPro.italic": true,
    "workbench.colorTheme": "Andromeda Italic",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "\"Dank Mono\", \"FuraCode Nerd Font\"",
    "diffEditor.ignoreTrimWhitespace": false,
    "todo-tree.tree.showScanModeButton": false,
    "eslint.alwaysShowStatus": true,
    "stylelint.packageManager": "yarn",
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "html-css-class-completion.enableEmmetSupport": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "prettier.singleQuote": true,
    "emmet.triggerExpansionOnTab": true,
    "vs-kubernetes": {
      "vs-kubernetes.helm-path.mac": "/Users/jturner/.vs-kubernetes/tools/helm/darwin-amd64/helm",
      "vs-kubernetes.kubectl-path.mac": "/Users/jturner/.vs-kubernetes/tools/kubectl/kubectl",
      "vs-kubernetes.minikube-path.mac": "/Users/jturner/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
      "vs-kubernetes.draft-path.mac": "/Users/jturner/.vs-kubernetes/tools/draft/darwin-amd64/draft"
    }
}
